{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ExecuteMsg",
  "oneOf": [
    {
      "description": "Creates a new vault by calling the instantiate method of the VAULT_CONTRACT, which returns a contract address, that is then associated with the `msg.sender`",
      "type": "object",
      "required": [
        "create_vault"
      ],
      "properties": {
        "create_vault": {
          "type": "object"
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Creates a new LP_GROUP by calling the instantiate method of the LP_GROUP_CONTRACT, which returns a contract address, that is then associated with the `msg.sender`.",
      "type": "object",
      "required": [
        "create_l_p_group"
      ],
      "properties": {
        "create_l_p_group": {
          "type": "object"
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Withdraw generated fees to the address provided by the owner",
      "type": "object",
      "required": [
        "withdraw_fees"
      ],
      "properties": {
        "withdraw_fees": {
          "type": "object",
          "required": [
            "to_address"
          ],
          "properties": {
            "to_address": {
              "type": "string"
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Allows the current owner to transfer ownership to another user.",
      "type": "object",
      "required": [
        "transfer"
      ],
      "properties": {
        "transfer": {
          "type": "object",
          "required": [
            "to_address"
          ],
          "properties": {
            "to_address": {
              "type": "string"
            }
          }
        }
      },
      "additionalProperties": false
    }
  ]
}
